{
  "name": "@bingtsingw/orchid-helper",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check-type": "tsc --noEmit",
    "dev": "tsup --watch",
    "lint:js": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": "@xstools-dev/eslint-config/base"
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "orchid-orm": "^1.35.0"
  },
  "peerDependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "orchid-orm": "^1.37.7"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true,
    "minify": true
  }
}
