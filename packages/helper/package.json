{
  "name": "@bingtsingw/orchid-helper",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bingtsingw/orchid-helper.git",
    "directory": "packages/helper"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "check-type": "tsc --noEmit",
    "dev": "tsup --watch",
    "lint:js": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:js:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix"
  },
  "eslintConfig": {
    "extends": "@xstools-dev/eslint-config/base"
  },
  "dependencies": {
    "@xstools/utility": "^0.20.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "orchid-orm": "1.62.1"
  },
  "peerDependencies": {
    "orchid-orm": "^1.62.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true,
    "minify": true
  }
}
